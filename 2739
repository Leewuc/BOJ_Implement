#include <iostream>
using namespace std;
int main(){
   int a;
   cin >> a;
   cout << a << " * 1 = " << a << '\n';
   cout << a << " * 2 = " << a*2 << '\n'; 
   cout << a << " * 3 = " << a*3 << '\n';
   cout << a << " * 4 = " << a*4 << '\n'; 
   cout << a << " * 5 = " << a*5 << '\n'; 
   cout << a << " * 6 = " << a*6 << '\n'; 
   cout << a << " * 7 = " << a*7 << '\n'; 
   cout << a << " * 8 = " << a*8 << '\n'; 
   cout << a << " * 9 = " << a*9 << '\n'; 
}

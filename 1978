#include <iostream>
using namespace std;
int a;
void prime(int n){
    if(n < 2){
        return ;
    }
    for(int i=2;i*i<=n;i++){
        if(n % i == 0) return;
}
    a++;
}
int main(){
    int N;
    cin >> N;
    for(int i=0; i< N; i++){
        int n;
        cin >> n;
        prime(n);
    }
    cout << a;
}

#include <iostream>
#include <vector>
using namespace std;
int a;
int main(){
    cin >> a;
    vector<int> n;
    while(a--){
        int b=0;
        cin >> b;
        n.push_back(b);
    }
    int c=0;
    int d=0;
    cin >> c;
    for(int i=0;i<n.size();i++){
        if(n[i] == c) d++;
    }
    cout << d << endl;
}
